public class 2022_66rec {

	public static void main(String[] args) {
	      //driver//
		     int arr[][]=   { 
		    		 {12, 22, 23, 54, 11},
		    		 {43, 35, 21, 20, 30},
		    		 {34, 23, 43, 22, 30},
		    		 {25, 31, 2, 20, 34},
		    		 {10, 22, 10, 11, 10},
		    		 {40, 13, 3, 1, 23},

		     };
		 System.out.println(maxPath(arr));
          
	}
	public static int maxPath (int [][] arr)
	{
		if (maxPath(arr, 0, 0, 0)==arr[0][0]) return Integer.MIN_VALUE;
		else return maxPath(arr, 0, 0, 0);
		
	}
	private static int maxPath(int[][] arr, int i, int j,int sum) {
		
		if (i<0 || i>arr.length-1 || j<0 || j>arr[0].length-1 || arr[i][j]==-1) return 0;
		if (i==arr.length-1 && j==arr.length-1) return 1;
		
		int units=arr[i][j]%10, tens=arr[i][j]/10;
		int a=maxPath(arr, i+tens, j+units, sum+arr[i][j] ) +arr[i][j] ;
		int b=maxPath(arr, i+units, j+tens, sum+arr[i][j] ) +arr[i][j] ;
		return  Math.max(a, b);
	 
	 
 
	}
